{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "DeploymentID": {
      "type": "string"
    }
  },

  "variables": {
    "AxoniusPortal": "https://training-demo.on.axonius.com/",
    "AxoniusUserName": "admin",
    "AxoniusPassword": "admin"

  },
    "resources": [],
  "outputs": {
    "Axonius Portal": {
      "type": "String",
